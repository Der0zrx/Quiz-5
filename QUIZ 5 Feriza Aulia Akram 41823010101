#VARIABLE
p = 11
q = 5
r = 4

#Ekspresi boolean

a = ((p - r) == (r + q))
b = (((p % 3) + q) != (r % 2))
c = ((q - 3) == (p % 2 + q))
d = ((r + q) != ((p * 2) % 2))
e = ((((q % 3) + p) > (r % 2)))
f = (((r + p)) <= (q * 5))

print("a =", a)
print("b =", b)
print("c =", c)
print("d =", d)
print("e =", e)
print("f =", f)

