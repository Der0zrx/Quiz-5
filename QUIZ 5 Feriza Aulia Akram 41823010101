print('7. Pembagi Indeks')
def pembagi_indeks1(nums, divisor):
    for i in range(len(nums)):
        if nums[i] % divisor == 0:
            return i

vals = [101,4,12,24]
print(pembagi_indeks1(vals, 2))
print('')


print("\n 8. ")
def mystery(n,m):
    p = 0
    e = 0
    while p < n:
        p = p + 1
        e = 9
    return p
print(mystery(4, 3))



